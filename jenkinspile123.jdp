pipeline{
    agent any
    stages{
        stage('Run on PR'){
             when {
                expression { env.CHANGE_ID ==~ /.*/ }
            }
            steps{
                echo "this is a pull request"
            }
        }
        stage('Merge Pull '){
            steps{
                $class: 'GhprbPullRequestMerge'
            }
        }
    }   
}        
    
