node {
   
   stage('SCM Checkout'){
    url: 'https://github.com/javahometech/myweb'
   }
   stage('text'){
   echo "navya"
   sh 'pwd'
   
   }
 
   stage('Email Notification'){
		mail bcc: '', body: '''Hai, welcome to Jenkins email alerts
    Thanks
    Navya''', cc: '', from: '', replyTo: '', subject: 'Jenkins job', to: 'navyasree068@gmail.com'
   
   }
}
